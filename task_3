Выведите элементы этого массива в виде HTML таблицы.

let employees = [
	{name: 'employee1', age: 30, salary: 400},
	{name: 'employee2', age: 31, salary: 500},
	{name: 'employee3', age: 32, salary: 600},
];

let table=document.querySelector('#table')

for ( let newEmployee of employees){
    let tr=document.createElement('tr')

    let trName=document.createElement('td')
    trName.innerHTML=newEmployee.name
    tr.appendChild(trName)
    
    let trAge=document.createElement('td')
    trAge.innerHTML=newEmployee.age
    tr.appendChild( trAge)
    
    let trSalary=document.createElement('td')
    trSalary.innerHTML=newEmployee.salary
    tr.appendChild(trSalary)

    table.appendChild(tr)
}
